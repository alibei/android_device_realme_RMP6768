# Graphics
genfscon sysfs /devices/platform/13040000.mali                                                                    u:object_r:sysfs_mali:s0

# OTG
genfscon sysfs /class/power_supply/usb/otg_switch                                                                 u:object_r:sysfs_otg_switch:s0
genfscon sysfs /devices/platform/charger/power_supply/usb/otg_switch                                              u:object_r:sysfs_otg_switch:s0

# Performance
genfscon sysfs /devices/system/cpu/cpufreq/mtk/.cluster_(min|max)_freq                                            u:object_r:sysfs_mtk_cpufreq:s0
genfscon sysfs /kernel/gpu/gpu_(min|max)_clock                                                                    u:object_r:sysfs_mtk_gpufreq:s0

# Touchpanel
genfscon proc  /touchpanel                                                                                        u:object_r:proc_tp:s0

# Wakeup (https://lkml.org/lkml/2019/8/6/1275)
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt6358-rtc/wakeup                      u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt635x-auxadc/wakeup                   u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/1000d000.pwrap/1000d000.pwrap:mt6358-pmic/mt6358-rtc/rtc/rtc0/wakeup             u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/10238000.gce/wakeup                                                              u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11004000.i2c7/i2c-7/7-0037/wakeup                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11004000.i2c7/i2c-7/7-004e/tcpc/type_c_port0/dual-role-type_c_port0/wakeup       u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11016000.i2c5/i2c-5/5-006b/wakeup                                                u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/charger/power_supply/ac/wakeup                                                   u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/charger/power_supply/battery/wakeup                                              u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/charger/power_supply/usb/wakeup                                                  u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/charger/wakeup                                                                   u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/odm/odm:hall_switch/wakeup                                                       u:object_r:sysfs_wakeup:s0
